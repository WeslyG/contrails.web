// @flow
declare module 'reelm' {
    declare type Dispatch = (action: Object) => any;
    declare type ReduxEnhancher = any;

    declare type Effect = any;
    declare type ReelmEffectHandler = (effect: Effect) => any | Promise<any>;
    declare function forwardTo(dispatch: Dispatch, ...path: string[]): Dispatch;
    declare function reelmRunner(...args: *[]): ReduxEnhancher;
    declare function spoiled(state: any, effects: () => any): any;
}

declare module 'reelm/effects' {
    declare var effectType: { [key: string]: string };
    declare function fork(generator: any): any;
    declare function put(action: any): any;
    declare function select(selector?: any): any;
    declare function call(generator: any): any;
    declare function unprocessed(): any;
}
