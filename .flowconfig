[ignore]
.*/fbjs/.*
.*/radium/.*
.*/flux/.*
.*/retail-ui/lib/.*
.*/retail-ui/docs/.*
.*/retail-ui/scripts/.*
.*/retail-ui/testing/.*
.*/retail-ui/components/.*
.*/react-ui-validations/src/.*
.*/stylelint/.*
.*/retail-ui/docs/stories/.*
.*/storypage/.*
.*/flow-coverage-report/.*
.*/immutable/.*
.*/react-draggable/.*

[include]

[libs]
interfaces/

[options]
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=local_modules

module.name_mapper='^.*\.less+$' -> '<PROJECT_ROOT>/flow/StylesFlowStub'
module.name_mapper='^ui$' -> '<PROJECT_ROOT>/src/commons/ui/index.js'
module.name_mapper='^\(commons/.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^.*Responses.*\.json$' -> '<PROJECT_ROOT>/flow/ResponsesFlowStub'

esproposal.class_instance_fields=enable
unsafe.enable_getters_and_setters=true

suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
suppress_comment= \\(.\\|\n\\)*\\@flow\\-disable\\-next\\-line
