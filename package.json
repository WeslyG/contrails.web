{
  "name": "contrail-front",
  "version": "0.0.1",
  "description": "Front end app for vostok-contrail",
  "main": "index.js",
  "author": "tihonove",
  "license": "MIT",
  "dependencies": {
    "@skbkontur/react-ui": "^0.9.6",
    "@types/react": "^16.0.17",
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "classnames": "^2.2.5",
    "classnames-loader": "^2.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "decimal.js": "^7.3.0",
    "eslint": "^4.3.0",
    "eslint-config-skbkontur-retail": "^3.0.2",
    "eslint-import-resolver-webpack": "^0.8.3",
    "extract-text-webpack-plugin": "^3.0.1",
    "flow-bin": "^0.56.0",
    "flow-coverage-report": "^0.3.0",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "less": "2.x",
    "less-loader": "^4.0.5",
    "mocha": "^4.0.1",
    "mocha-webpack": "^0.7.0",
    "moment": "^2.18.1",
    "postcss-loader": "^2.0.8",
    "prettier": "^1.5.3",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-draggable": "^3.0.3",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-declaration-strict-value": "^1.0.4",
    "stylelint-declaration-use-variable": "^1.6.0",
    "stylelint-order": "^0.7.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-node-externals": "^1.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.8",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --env.apiTarget=logsearch --env.apiMode=fake",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006",
    "flow": "flow",
    "flow.check": "flow check",
    "flow.coverage-report": "flow-coverage-report -t html",
    "travis:flow.coverage-report": "flow-coverage-report -t text --threshold 99",
    "lint": "eslint src stories",
    "stylelint": "stylelint src/**/*.less stories/**/*.less --syntax less",
    "test": "cross-env NODE_ENV=mocha mocha-webpack --opts test/mocha.opts test/**/*.test.js",
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --env.apiTarget=vostok",
    "logsearch:build": "cross-env NODE_ENV=production webpack --env.apiTarget=logsearch",
    "logsearch:local-staging:build": "cross-env NODE_ENV=production webpack --env.apiTarget=logsearch --env.addIISWebConfig=true",
    "vendor:clean": "rimraf prebuild",
    "vendor:build": "rimraf prebuild & webpack --config webpack.vendor.dll.config.js"
  },
  "flow-coverage-report": {
    "includeGlob": [
      "src/**/*.js",
      "src/**/*.jsx",
      "stories/**/*.js",
      "stories/**/*.jsx",
      "test/**/*.js"
    ],
    "excludeGlob": [
      "src/Domain/Responses/index.js",
      "src/commons/ApiInjection.js",
      "src/commons/ApplicationError.js",
      "src/Domain/ContrailsApi.js"
    ]
  }
}
